algoritmo "Creuza02"
// Função :
// Autor :
// Data : 5/20/2020
// Seção de Declarações 
var
   moeda_brl, moeda_dolar: real
   
inicio
      Escreva("Quantos reais eu tenho? R$")
      Leia(moeda_brl)
      moeda_dolar <- 2.22
      Escreva("Eu tenho USD$", moeda_brl/moeda_dolar, " dolares")
fimalgoritmo
