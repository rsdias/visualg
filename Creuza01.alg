algoritmo "Creuza01"
// Função :
// Autor :
// Data : 5/20/2020
// Seção de Declarações 
var
   ano_atual, ano_nasc, idade: inteiro
inicio
      Escreva("Em que ano estamos? ")
      Leia(ano_atual)
      Escreva("Em que ano eu nasci? ")
      Leia(ano_nasc)
      idade <- ano_atual - ano_nasc
      Escreva("Eu tenho ", idade, " anos de idade!")
fimalgoritmo
