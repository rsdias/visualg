algoritmo "semnome"
// Função :
// Autor :
// Data : 6/22/2021
// Seção de Declarações 
var
a, val, conta_dez: inteiro

inicio

Para a <- 1 ate 3 faca
    Escreva("Digite um valor: ")
    Leia(val)
    Se (val>0) e (val <=10) entao
       conta_dez <- conta_dez + 1
    FimSe
FimPara
Escreval("O total de valores entre 1 e 3 eh de ", conta_dez)
// Seção de Comandos 
fimalgoritmo
